generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model tesoros {
  id        String   @id(map: "Tesoro_pkey") @default(dbgenerated("uuid_generate_v4()"))
  name      String
  condition String[]
  origin    String?
  brand     String?
  material  String[]
  category  String?
  large     Int?
  width     Int?
  height    Int?
  diameter  Int?
  images    String[]
  units     Int
  price     Float
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model stream {
  id       BigInt   @id(map: "streams_pkey") @unique @default(autoincrement())
  time     DateTime @db.Time(6)
  duration Int      @db.SmallInt
  date     DateTime @db.Date
}
